{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "jika-blocks/real-time-stock-price-chart",
  "version": "0.1.0",
  "title": "Real-Time Stock Price Chart",
  "category": "widgets",
  "icon": "gutenberg-jika-widgets-icon gutenberg-jika-widgets-real-time-stock-price-chart",
  "description": "With our stock ticker widgets, you can easily display real-time stock prices and updates on your website or blog. Simply choose the stock you want to track, and customize the widget to match your content.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "src": {
      "type": "string"
    },
    "height": {
      "type": "integer"
    },
    "auto": {
      "type": "boolean",
      "default": true
    },
    "background": {
      "type": "string",
      "default": "#FFFFFF"
    }
  },
  "textdomain": "jika-widgets",
  "editorScript": "file:../../index.js",
  "editorStyle": "file:../../index.css",
  "style": "file:../../style-index.css"
}